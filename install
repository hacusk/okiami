#!/bin/sh
mkdir -p ~/.config/
## alacritty
ln -sf ~/okiami/alacritty ~/.config/alacritty
## amethyst
ln -sf ~/okiami/amethyst ~/.config/amethyst
## fish
ln -sf ~/okiami/fish ~/.config/fish
## git
ln -sf ~/okiami/git ~/.config/git
## zsh
ln -sf ~/okiami/zsh/.zshrc ~/.zshrc
## tmux
ln -sf ~/okiami/tmux ~/.config/tmux
## nvim
ln -sf ~/okiami/nvim ~/.config/nvim
